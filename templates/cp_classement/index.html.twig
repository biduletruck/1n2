{% extends 'base.html.twig' %}

{% block title %}CPClassement index{% endblock %}

{% block body %}
    <h1>CPClassement index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ClassementPhoto</th>
                <th>NombrePoints</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for c_p_classement in c_p_classements %}
            <tr>
                <td>{{ c_p_classement.id }}</td>
                <td>{{ c_p_classement.ClassementPhoto }}</td>
                <td>{{ c_p_classement.NombrePoints }}</td>
                <td>{{ c_p_classement.UpdatedAt ? c_p_classement.UpdatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_c_p_classement_show', {'id': c_p_classement.id}) }}">show</a>
                    <a href="{{ path('app_c_p_classement_edit', {'id': c_p_classement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_c_p_classement_new') }}">Create new</a>
{% endblock %}
